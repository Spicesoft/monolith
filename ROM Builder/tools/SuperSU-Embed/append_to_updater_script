#SuperSU install (supersu)
ui_print("Installing SuperSU ..."); #supersu
package_extract_dir("supersu", "/tmp/supersu"); #supersu
run_program("/sbin/busybox", "unzip", "/tmp/supersu/supersu.zip", "META-INF/com/google/android/*", "-d", "/tmp/supersu"); #supersu
run_program("/sbin/busybox", "sh", "/tmp/supersu/META-INF/com/google/android/update-binary", "dummy", "1", "/tmp/supersu/supersu.zip"); #supersu